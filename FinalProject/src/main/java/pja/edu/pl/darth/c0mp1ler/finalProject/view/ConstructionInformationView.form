<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="pja.edu.pl.darth.c0mp1ler.finalProject.view.ConstructionInformationView">
  <grid id="27dc6" binding="panel" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <font/>
    </properties>
    <border type="empty">
      <size top="20" left="20" bottom="20" right="20"/>
    </border>
    <children>
      <component id="dfb11" class="javax.swing.JTextPane" binding="constructionInfo">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
          <gridbag weightx="1.0" weighty="1.0"/>
        </constraints>
        <properties>
          <editable value="false"/>
          <font name="Consolas" size="16"/>
        </properties>
      </component>
      <grid id="214bb" layout-manager="GridLayoutManager" row-count="4" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="1.0"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <scrollpane id="ffae5">
            <constraints>
              <grid row="0" column="0" row-span="3" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font name="Consolas" size="14"/>
            </properties>
            <border type="line" title="Buildings:">
              <font name="BIZ UDGothic" size="18"/>
              <title-color color="-10461088"/>
              <size top="10" left="10" bottom="10" right="10"/>
              <color color="-16777216"/>
            </border>
            <children>
              <component id="983c0" class="javax.swing.JList" binding="buildingList" custom-create="true">
                <constraints/>
                <properties>
                  <font name="Consolas" size="14"/>
                  <selectionBackground color="-7828559"/>
                </properties>
              </component>
            </children>
          </scrollpane>
          <grid id="1a52b" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints>
              <grid row="0" column="1" row-span="3" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties/>
            <border type="empty">
              <size top="15" left="15" bottom="15" right="15"/>
            </border>
            <children>
              <grid id="54b69" layout-manager="GridLayoutManager" row-count="3" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="0" left="0" bottom="0" right="0"/>
                <constraints border-constraint="West"/>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="e29b9" class="javax.swing.JButton" binding="addBuilding">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <background color="-8091510"/>
                      <font name="Consolas" size="20" style="1"/>
                      <text value="+"/>
                    </properties>
                  </component>
                  <component id="38f48" class="javax.swing.JButton" binding="removeBuilding">
                    <constraints>
                      <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <background color="-8091510"/>
                      <font name="Consolas" size="20" style="1"/>
                      <text value="-"/>
                    </properties>
                  </component>
                  <vspacer id="7ac9">
                    <constraints>
                      <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
                    </constraints>
                  </vspacer>
                </children>
              </grid>
              <grid id="f480a" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="0" left="0" bottom="0" right="0"/>
                <constraints border-constraint="South"/>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="2784a" class="javax.swing.JButton" binding="removeConstruction" custom-create="true">
                    <constraints>
                      <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <background color="-1329715"/>
                      <enabled value="true"/>
                      <font name="BIZ UDGothic" size="14"/>
                      <hideActionText value="true"/>
                      <text value="REMOVE CONSTRUCTION"/>
                    </properties>
                  </component>
                  <hspacer id="5ba0c">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                  </hspacer>
                </children>
              </grid>
            </children>
          </grid>
          <hspacer id="84e10">
            <constraints>
              <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
          </hspacer>
        </children>
      </grid>
    </children>
  </grid>
</form>
